@model IEnumerable<crssAssetDV.Models.Loans>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.LoanType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Location)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SuppliedPowerBlock)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SuppliedCable)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SuppliedCover)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.oampsUpdated)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.intuneUpdated)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.edquipSigned)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.LoanType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Location)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SuppliedPowerBlock)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SuppliedCable)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SuppliedCover)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.oampsUpdated)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.intuneUpdated)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.edquipSigned)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
